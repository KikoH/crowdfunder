<%= form_for @project do |f| %>
	<%= f.text_field :name, placeholder: 'Name' %>
	<br><%= f.text_area :description, placeholder: 'Description' %>
	<br><%= f.date_select :start_date %>
	<br><%= f.date_select :end_date %>
	<br><%= f.number_field :funding_goal, placeholder: 'Funding Goal' %>
	<br><%= f.submit %>	
<% end %>